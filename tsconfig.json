{
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"allowJs": true,
		"noEmit": true,
		"target": "ES2024",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"paths": {
		}
	},
	"include": [
		"**/*.ts",
		"**/*.mts",
		"**/*.js",
		"**/*.mjs"
	]
}