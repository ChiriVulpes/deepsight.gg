{
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"allowJs": true,
		"noEmit": true,
		"target": "ES2020",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"paths": {
			"bungie-api-ts/*": ["./src/node_modules/bungie-api-ts/*"],
			"@app/utility/*": ["./src/utility/*"]
		}
	}
}